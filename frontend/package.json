{
  "name": "healthcare-management-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Patient records management system frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && NODE_ENV=production tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "dev:full": "npm run dev & npm run test -- --watch",
    "preview:prod": "NODE_ENV=production npm run build && npm run preview",
    "audit": "npm audit",
    "generate:component": "node ./scripts/generate-component.js",
    "clean": "rimraf dist/*"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.8.0",
    "@mui/x-date-pickers": "^8.8.0",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@types/dompurify": "^3.0.5",
    "antd": "^5.26.3",
    "axios": "^1.10.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "libphonenumber-js": "^1.12.10",
    "mui-tel-input": "^9.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-router-dom": "^6.30.1",
    "vitest": "^3.2.4",
    "zod": "^4.0.17",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@hookform/resolvers": "^5.2.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/zxcvbn": "^4.4.5",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
